#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5d1cb956a320 .scope module, "tb_and" "tb_and" 2 1;
 .timescale 0 0;
v0x5d1cb9579a50_0 .var "a", 0 0;
v0x5d1cb9579b20_0 .var "b", 0 0;
v0x5d1cb9579bf0_0 .net "y", 0 0, L_0x5d1cb9579cf0;  1 drivers
S_0x5d1cb956a4b0 .scope module, "uui" "and_gate" 2 6, 3 1 0, S_0x5d1cb956a320;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5d1cb9579cf0 .functor AND 1, v0x5d1cb9579a50_0, v0x5d1cb9579b20_0, C4<1>, C4<1>;
v0x5d1cb9531490_0 .net "a", 0 0, v0x5d1cb9579a50_0;  1 drivers
v0x5d1cb9579870_0 .net "b", 0 0, v0x5d1cb9579b20_0;  1 drivers
v0x5d1cb9579930_0 .net "y", 0 0, L_0x5d1cb9579cf0;  alias, 1 drivers
    .scope S_0x5d1cb956a320;
T_0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5d1cb9579a50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5d1cb9579b20_0, 0, 1;
    %delay 3, 0;
    %vpi_call 2 12 "$display", "y=%b", v0x5d1cb9579bf0_0 {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
