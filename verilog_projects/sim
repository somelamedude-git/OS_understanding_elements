 .timescale 0 0;
v0x5eb57f8ef360_0 .var "data", 31 0;
v0x5eb57f938ec0_0 .var/i "i", 31 0;
    .scope S_0x5eb57f8ef1d0;
T_0 ;
    %pushi/vec4 4207856382, 0, 32;
    %store/vec4 v0x5eb57f8ef360_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5eb57f938ec0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5eb57f938ec0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x5eb57f8ef360_0;
    %load/vec4 v0x5eb57f938ec0_0;
    %muli 8, 0, 32;
    %part/s 8;
    %vpi_call 2 9 "$display", "data[8*%0d +: 8] = 0x%0h", v0x5eb57f938ec0_0, S<0,vec4,u8> {1 0 0};
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5eb57f938ec0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5eb57f938ec0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 2 12 "$display", "data[7:0]   = 0x%0h", &PV<v0x5eb57f8ef360_0, 0, 8> {0 0 0};
    %vpi_call 2 13 "$display", "data[15:8]  = 0x%0h", &PV<v0x5eb57f8ef360_0, 8, 8> {0 0 0};
    %vpi_call 2 14 "$display", "data[23:16] = 0x%0h", &PV<v0x5eb57f8ef360_0, 16, 8> {0 0 0};
    %vpi_call 2 15 "$display", "data[31:24] = 0x%0h", &PV<v0x5eb57f8ef360_0, 24, 8> {0 0 0};
    %end;
    .thread T_0;
