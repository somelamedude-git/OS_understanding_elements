#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5a0df115ba60 .scope module, "tb_dff" "tb_dff" 2 1;
 .timescale 0 0;
v0x5a0df116f140_0 .var "clk", 0 0;
v0x5a0df116f200_0 .var "d", 0 0;
v0x5a0df116f2d0_0 .net "q", 0 0, v0x5a0df116ef00_0;  1 drivers
v0x5a0df116f3d0_0 .var "rstn", 0 0;
S_0x5a0df115bbf0 .scope module, "uut" "dff" 2 7, 3 1 0, S_0x5a0df115ba60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "rstn";
    .port_info 2 /INPUT 1 "clk";
    .port_info 3 /OUTPUT 1 "q";
v0x5a0df114a260_0 .net "clk", 0 0, v0x5a0df116f140_0;  1 drivers
v0x5a0df116ee40_0 .net "d", 0 0, v0x5a0df116f200_0;  1 drivers
v0x5a0df116ef00_0 .var "q", 0 0;
v0x5a0df116efd0_0 .net "rstn", 0 0, v0x5a0df116f3d0_0;  1 drivers
E_0x5a0df11236a0 .event posedge, v0x5a0df114a260_0;
    .scope S_0x5a0df115bbf0;
T_0 ;
    %wait E_0x5a0df11236a0;
    %load/vec4 v0x5a0df116efd0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5a0df116ef00_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5a0df116ee40_0;
    %assign/vec4 v0x5a0df116ef00_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5a0df115ba60;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a0df116f140_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x5a0df115ba60;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5a0df116f140_0;
    %inv;
    %store/vec4 v0x5a0df116f140_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5a0df115ba60;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a0df116f3d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a0df116f200_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a0df116f3d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a0df116f200_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5a0df116f200_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5a0df116f200_0, 0, 1;
    %delay 20, 0;
    %vpi_call 2 17 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5a0df115ba60;
T_4 ;
    %wait E_0x5a0df11236a0;
    %vpi_call 2 21 "$display", "Time=%0t, clk=%b, d=%b, q=%b", $time, v0x5a0df116f140_0, v0x5a0df116f200_0, v0x5a0df116f2d0_0 {0 0 0};
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
